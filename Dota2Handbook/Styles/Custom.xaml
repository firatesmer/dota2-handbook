    <x:Double x:Key="NarrowMinWidth">0</x:Double>
    <x:Double x:Key="NormalMinWidth">521</x:Double>
    <x:Double x:Key="WideMinWidth">1200</x:Double>

    <Color x:Key="CustomColor">Black</Color>
    <Color x:Key="ContrastColor">White</Color>
    <Color x:Key="SystemAccentColor">Black</Color>

    <SolidColorBrush x:Key="SystemControlHighlightListAccentLowBrush" Color="LightGray" Opacity="0.3"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentMediumBrush" Color="LightGray" Opacity="0.7" />
    <SolidColorBrush x:Key="SystemControlHighlightListMediumBrush" Color="LightGray" Opacity="0.7" />
    <SolidColorBrush x:Key="SystemControlHighlightListAccentHighBrush" Color="LightGray" Opacity="0.8" />

    <SolidColorBrush x:Key="ToggleSwitchFillOn" Color="Gray" />
    
